pico-8 cartridge // http://www.pico-8.com
version 42
__lua__

function _init()
 torpx={}
 torpy={}
 torpspr={} 
 
 torpx[1]=64
 torpy[1]=120
 torpspr[1]=1
 debug=torpy[1]
end


function _draw()
 cls(0)
 print(debug)
 spr(torpspr[1],torpx[1],torpy[1])
end

function _update()
 if (torpspr[1] < 3) then 
  torpspr[1]=torpspr[1]+1
 else
  torpspr[1]=1
 end
 
 if (torpy[1] < 121 and torpy[1] > 10 ) then
 	torpy[1] = torpy[1] - 3
 else 
  torpy[1] = 120
 end
end

